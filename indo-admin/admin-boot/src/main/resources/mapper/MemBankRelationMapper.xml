<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.indo.admin.modules.mem.mapper.MemBankRelationMapper">

    <select id="queryList" resultType="com.indo.admin.modules.mem.vo.MemBankRelationVO">
        SELECT * FROM mem_bank_relation mb
        <where>
            <if test="req.memId !=null">
                and mb.memId = #{req.memId}
            </if>
            <if test="req.keyword !=null and req.keyword !=''">
                and mb.city like concat('%', #{req.keyword,jdbcType=VARCHAR}, '%')
            </if>
            <if test="req.startTime != null and req.startTime != ''">
                and mb.create_time &gt;= req.startTime
            </if>
            <if test="req.endTime != null and req.endTime != ''">
                and mb.create_time &gt;= req.endTime
            </if>
        </where>
        ORDER BY mb.create_time desc
    </select>
</mapper>
