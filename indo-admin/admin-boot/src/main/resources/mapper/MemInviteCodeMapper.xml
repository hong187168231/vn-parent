<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.indo.admin.modules.mem.mapper.MemInviteCodeMapper">

    <select id="queryList" resultType="com.indo.admin.modules.mem.vo.MemInviteCodeVo">
        SELECT * FROM mem_invite_code mb
        <where>
            <if test="req.memId !=null">
                and mb.mem_id = #{req.memId}
            </if>
            <if test="req.inviteCode !=null and req.inviteCode != '' ">
                and mb.inviteCode = #{req.inviteCode}
            </if>
            <if test="req.startTime != null and req.startTime != ''">
                and mb.create_time &gt;= req.startTime
            </if>
            <if test="req.endTime != null and req.endTime != ''">
                and mb.create_time &lt;= req.endTime
            </if>
        </where>
        ORDER BY mb.create_time desc
    </select>

</mapper>
